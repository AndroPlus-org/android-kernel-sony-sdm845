/ {
	reserved-memory {
		debug_region: debug_region@ffb00000 {
			compatible = "removed-dma-pool", "qcom,debug_memory";
			no-map;
			reg = <0 0xffb00000 0 0x100000>;
			label = "debug_mem";
		};

		ramoops: ramoops@ffc00000 {
			compatible = "removed-dma-pool", "ramoops";
			no-map;
			reg = <0 0xffc00000 0 0x00100000>;
			record-size = <0x1000>;
			console-size = <0x40000>;
			ftrace-size = <0x0>;
			msg-size = <0x0>;
			cc-size = <0x0>;
		};

		qseecom_region@0x8ab00000 {
			reg = <0 0x8ab00000 0 0x1900000>;
		};

		/delete-node/ camera_region@0x8bf00000;
		pil_camera_mem: camera_region@0x97c00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x97c00000 0 0x500000>;
		};
	};
};

&cam_cci {
	/* The value of clock that depens on the following parameter is different from Qualcomm.
	Change the value for Somc. */
	qcom,i2c_fast_mode {
		hw-thigh = <43>;
		hw-tlow = <64>;
		hw-tsu-sto = <41>;
		hw-tsu-sta = <41>;
		hw-thd-dat = <25>;
		hw-thd-sta = <35>;
		hw-tbuf = <64>;
		hw-scl-stretch-en = <0>;
		hw-trdhld = <6>;
		hw-tsp = <3>;
		cci-clk-src = <37500000>;
		status = "ok";
	};
	qcom,i2c_fast_plus_mode {
		hw-thigh = <16>;
		hw-tlow = <22>;
		hw-tsu-sto = <17>;
		hw-tsu-sta = <18>;
		hw-thd-dat = <16>;
		hw-thd-sta = <15>;
		hw-tbuf = <19>;
		hw-scl-stretch-en = <1>;
		hw-trdhld = <3>;
		hw-tsp = <3>;
		cci-clk-src = <37500000>;
		status = "ok";
	};

	qcom,cam-sensor@0 {
		status = "disable";
