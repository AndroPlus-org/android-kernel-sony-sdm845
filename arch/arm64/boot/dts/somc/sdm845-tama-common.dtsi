				"fpc1145_irq_active";

		pinctrl-0 = <&sdm_gpio_40>;
		pinctrl-1 = <&sdm_gpio_40_output_high>;
		pinctrl-2 = <&sdm_gpio_121>;
	};

	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
