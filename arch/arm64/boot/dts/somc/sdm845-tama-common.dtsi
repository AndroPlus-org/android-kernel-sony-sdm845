			    &sdm_gpio_5 &sdm_gpio_7 &sdm_gpio_8 &sdm_gpio_9
			    &sdm_gpio_10 &sdm_gpio_11_suspend &sdm_gpio_12 &sdm_gpio_13 &sdm_gpio_14
			    &sdm_gpio_15 &sdm_gpio_16 &sdm_gpio_17 &sdm_gpio_18 &sdm_gpio_19
			    &sdm_gpio_20 &sdm_gpio_21 &sdm_gpio_22_suspend &sdm_gpio_23 &sdm_gpio_24
			    &sdm_gpio_25 &sdm_gpio_26 &sdm_gpio_27 &sdm_gpio_28 &sdm_gpio_29
			    &sdm_gpio_30 &sdm_gpio_31 &sdm_gpio_32 &sdm_gpio_33 &sdm_gpio_34
			    &sdm_gpio_35 &sdm_gpio_36 &sdm_gpio_37 &sdm_gpio_39 &sdm_gpio_40
			    &sdm_gpio_41 &sdm_gpio_42 &sdm_gpio_43 &sdm_gpio_44 &sdm_gpio_49
			    &sdm_gpio_50 &sdm_gpio_52 &sdm_gpio_55 &sdm_gpio_56 &sdm_gpio_57
			    &sdm_gpio_58 &sdm_gpio_59 &sdm_gpio_60 &sdm_gpio_62
			    &sdm_gpio_63 &sdm_gpio_69 &sdm_gpio_75 &sdm_gpio_76 &sdm_gpio_77
			    &sdm_gpio_78 &sdm_gpio_79 &sdm_gpio_80 &sdm_gpio_87 &sdm_gpio_88
			    &sdm_gpio_89 &sdm_gpio_90 &sdm_gpio_91
			    &sdm_gpio_92 &sdm_gpio_93 &sdm_gpio_94 &sdm_gpio_95 &sdm_gpio_96
			    &sdm_gpio_102 &sdm_gpio_103 &sdm_gpio_104
			    &sdm_gpio_105 &sdm_gpio_106 &sdm_gpio_107 &sdm_gpio_108 &sdm_gpio_109
			    &sdm_gpio_110 &sdm_gpio_111 &sdm_gpio_112 &sdm_gpio_113 &sdm_gpio_114
			    &sdm_gpio_115 &sdm_gpio_116 &sdm_gpio_120 &sdm_gpio_121 &sdm_gpio_122
			    &sdm_gpio_123 &sdm_gpio_124 &sdm_gpio_125 &sdm_gpio_126 &sdm_gpio_128
			    &sdm_gpio_129>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
