	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pm8998_gpio_1 &pm8998_gpio_2 &pm8998_gpio_4 &pm8998_gpio_5
			    &pm8998_gpio_6 &pm8998_gpio_7 &pm8998_gpio_8 &pm8998_gpio_9 &pm8998_gpio_10
			    &pm8998_gpio_11 &pm8998_gpio_12 &pm8998_gpio_13 &pm8998_gpio_14 &pm8998_gpio_15
			    &pm8998_gpio_16 &pm8998_gpio_17 &pm8998_gpio_18 &pm8998_gpio_19 &pm8998_gpio_20
			    &pm8998_gpio_21
			    &pmi8998_gpio_1 &pmi8998_gpio_2 &pmi8998_gpio_3 &pmi8998_gpio_5 &pmi8998_gpio_6
			    &pmi8998_gpio_8 &pmi8998_gpio_9 &pmi8998_gpio_10 &pmi8998_gpio_11
			    &pmi8998_gpio_12 &pmi8998_gpio_14 &pm8005_gpio_1 &pm8005_gpio_2>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 124 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	sim_detect {
		compatible = "sim-detect";

		sim1_det {
			label = "sim-detection";
			gpios = <&tlmm 112 0x0>;
			debounce-interval = <10>;
		};
	};

	/* I2C: QUP5 */
	i2c@894000 {
		pinctrl-0 = <&sdm_gpio_85 &sdm_gpio_86>;
		pinctrl-1 = <&sdm_gpio_85_suspend &sdm_gpio_86_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* UART: QUP6 */
	qcom,qup_uart@0x898000 {
		clock-frequency = <4000000>;
		status = "okay";
	};

	/* SPI: QUP8 */
	spi@a80000 {
		status = "okay";
	};

	/* UART: QUP9 */
	qcom,qup_uart@0xa84000 {
		pinctrl-0 = <&sdm_gpio_4 &sdm_gpio_5>;
		pinctrl-1 = <&sdm_gpio_4 &sdm_gpio_5>;
		status = "okay";
	};

	/* I2C: QUP10 */
	i2c@a88000 {
		pinctrl-0 = <&sdm_gpio_55 &sdm_gpio_56>;
		pinctrl-1 = <&sdm_gpio_55_suspend &sdm_gpio_56_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* I2C: QUP14 */
	i2c@a98000 {
		pinctrl-0 = <&sdm_gpio_33 &sdm_gpio_34>;
		pinctrl-1 = <&sdm_gpio_33_suspend &sdm_gpio_34_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
		tcs3490@72 {
			compatible = "ams,tcs3490";
			reg = <0x72>;
			interrupt-parent = <&tlmm>;
			interrupts = <11 0x0>;
			rgbcir_vdd-supply = <&pm8998_l22>;
			rgbcir_vio-supply = <&cam_vio_vreg>;
			pinctrl-names = "rgbcir_irq_active", "rgbcir_irq_suspend";
			pinctrl-0 = <&sdm_gpio_11>;
			pinctrl-1 = <&sdm_gpio_11_suspend>;
			ams,rgbcir-vdd-supply = <1>;
			ams,rgbcir-gpio-vdd = <0>;
			ams,rgbcir-vio-supply = <1>;
		};
		tof_sensor@52 {
			compatible = "tof_sensor";
			reg = <0x52>;
			interrupt-parent = <&tlmm>;
			interrupts = <22 0x0>;
			tof_avdd-supply = <&pm8998_l22>;
			tof_vio-supply = <&cam_vio_vreg>;
			pinctrl-names = "tof_irq_active", "tof_irq_suspend";
			pinctrl-0 = <&sdm_gpio_22 &sdm_gpio_23>;
			pinctrl-1 = <&sdm_gpio_22_suspend &sdm_gpio_23>;
			tof-reset-gpio = <&tlmm 23 0>;
			sony,tof-sensor-name = "VL53L0";
			sony,tof-need-cam-on = <1>;
			sony,tof-avdd-supply = <1>;
			sony,tof-gpio-avdd = <0>;
			sony,tof-vio-supply = <1>;
			sony,tof-sensor-facing = <0>;
		};
	};
	cam_vio_vreg: cam_vio_vreg {
		compatible = "regulator-fixed";
		regulator-name = "camera_vio_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pmi8998_gpios 12 0>;
 	};

        qcom,wdt@17980000 {
		qcom,bark-time = <12000>;
	};

	sound-tavil {
		qcom,msm-mbhc-usbc-audio-supported = <1>;
		qcom,audio-routing =
			"AIF4 VI", "MCLK",
			"RX_BIAS", "MCLK",
			"MADINPUT", "MCLK",
			"AMIC2", "MIC BIAS2",
			"MIC BIAS2", "Headset Mic",
			"DMIC0", "MIC BIAS1",
			"MIC BIAS1", "Digital Mic0",
			"DMIC3", "MIC BIAS4",
			"MIC BIAS4", "Digital Mic3",
			"SpkrLeft IN", "SPK1 OUT",
			"SpkrRight IN", "SPK2 OUT";
	};

	rpmh-regulator-ldoa14 {
		pm8998_l14: regulator-l14 {
			qcom,proxy-consumer-voltage = <1800000>;
		};
	};

	sony_camera_module_0: sony,camera_modules@0 {
		cell-index = <0>;
		reg = <0x00>;
		interrupt-parent = <&tlmm>;
		status = "ok";
		compatible = "sony_camera_0";
		module_names = "SOI20BS2";
		default_module_name = "SOI20BS2";
		cam_vaf-supply = <&pm8998_l19>;
		cam_vio-supply = <&cam_vio_vreg>;
		cam_vana-supply = <&cam_vana_rear_verg>;
		cam_clk-supply = <&titan_top_gdsc>;
		cam-vreg-name = "cam_vaf", "cam_vio", "cam_vana", "cam_clk";
		cam-vreg-type = <0 0 0 0 0>;
		cam-vreg-min-voltage = <2700000 0 0 0>;
		cam-vreg-max-voltage = <2700000 0 0 0>;
		cam-vreg-op-mode = <300000 0 0 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active &sdm_gpio_21 &sdm_gpio_80>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend &sdm_gpio_21 &sdm_gpio_80>;
		gpios = <&tlmm 13 0>, <&tlmm 21 0>, <&tlmm 80 0>;
		gpio-req-tbl-num = <0 1 2>;
		gpio-req-tbl-flags = <1 1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK0", "SONY_CAMERA_SOF", "SONY_CAMERA_RESET";
		cci-master = <0>;
		clocks = <&clock_camcc CAM_CC_MCLK0_CLK>;
		clock-names = "cam_clk";
		clock-rates = <8000000>;
		use_spi = <0>;
		eeprom_addr = <0xA2>;
		eeprom_type = <2>;
		eeprom_max_len = <2048>;

		SOI20BS2 {
			i2c_freq_mode = <3>;
			power_off {
				commands =
					"gpio_reset",
					"cam_clk",
					"cam_vaf",
					"cam_vana",
					"cam_vio",
					"exit";
				gpio_reset = <5 0x0 0 5>;
				cam_clk = <4 0xffffffff 0 0>;
				cam_vaf = <3 0xffffffff 0 0>;
				cam_vana = <2 0xffffffff 0 5>;
				cam_vio = <1 0xffffffff 0 1>;
				exit = <14 0x0 0 0>;
			};
			power_on {
				commands =
					"cam_vana",
					"cam_vio",
					"cam_vaf",
					"cam_clk",
					"gpio_reset",
					"exit";
				cam_vana = <2 0 0 1>;
				cam_vio = <1 0 0 1>;
				cam_vaf = <3 2700 106500 3>;
				cam_clk = <4 0 0 1>;
				gpio_reset = <5 1 0 1>;
				exit = <14 0x0 0 0>;
			};
		};
	};

	sony_camera_module_1: sony,camera_modules@1 {
		cell-index = <1>;
		reg = <0x01>;
		interrupt-parent = <&tlmm>;
		status = "ok";
		compatible = "sony_camera_1";
		module_names = "LGI05BN1", "CHI05BN1";
		default_module_name = "CHI05BN1";
		cam_vdig-supply = <&pm8998_s3>;
		cam_vaf-supply = <&pm8998_l19>;
		cam_vio-supply = <&cam_vio_vreg>;
		cam_vana-supply = <&cam_vana_front_verg>;
		cam_clk-supply = <&titan_top_gdsc>;
		cam-vreg-name = "cam_vdig", "cam_vana", "cam_vio", "cam_clk";
		cam-vreg-type = <0 0 0 0>;
		cam-vreg-min-voltage = <1352000 2700000 0 0>;
		cam-vreg-max-voltage = <1352000 2700000 0 0>;
		cam-vreg-op-mode = <105000 150000 0 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_active &sdm_gpio_9>;
		pinctrl-1 = <&cam_sensor_mclk1_suspend &sdm_gpio_9>;
		gpios = <&tlmm 14 0>, <&tlmm 9 0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK2", "SONY_CAMERA_RESET";
		cci-master = <1>;
		clocks = <&clock_camcc CAM_CC_MCLK1_CLK>;
		clock-names = "cam_clk";
		clock-rates = <8000000>;
		use_spi = <0>;
		eeprom_addr = <0xA0>;
		eeprom_type = <2>;
		eeprom_max_len = <2048>;

		LGI05BN1 {
			i2c_freq_mode = <1>;
			power_off {
				commands =
					"gpio_reset",
					"cam_clk",
					"cam_vana",
					"cam_vio",
					"cam_vdig",
					"exit";
				cam_vdig = <0 0xffffffff 0 98>;
				cam_vio = <1 0xffffffff 0 1>;
				cam_vana = <2 0xffffffff 0 1>;
				gpio_reset = <5 0x0 0 1>;
				cam_clk = <4 0xffffffff 0 1>;
				exit = <14 0x0 0 0>;
			};
			power_on {
				commands =
					"cam_vdig",
					"cam_vio",
					"cam_vana",
					"cam_clk",
					"gpio_reset",
					"exit";
				cam_vdig = <0 1352 105000 1>;
				cam_vio = <1 0 0 1>;
				cam_vana = <2 0 0 1>;
				gpio_reset = <5 1 0 1>;
				cam_clk = <4 0 0 1>;
				exit = <14 0x0 0 0>;
			};
		};

		CHI05BN1 {
			i2c_freq_mode = <1>;
			power_off {
				commands =
					"gpio_reset",
					"cam_clk",
					"cam_vana",
					"cam_vio",
					"cam_vdig",
					"exit";
				cam_vdig = <0 0xffffffff 0 98>;
				cam_vio = <1 0xffffffff 0 1>;
				cam_vana = <2 0xffffffff 0 1>;
				gpio_reset = <5 0x0 0 1>;
				cam_clk = <4 0xffffffff 0 1>;
				exit = <14 0x0 0 0>;
			};
			power_on {
				commands =
					"cam_vdig",
					"cam_vio",
					"cam_vana",
					"cam_clk",
					"gpio_reset",
					"exit";
				cam_vdig = <0 1352 105000 1>;
				cam_vio = <1 0 0 1>;
				cam_vana = <2 0 0 1>;
				gpio_reset = <5 1 0 1>;
				cam_clk = <4 0 0 1>;
				exit = <14 0x0 0 0>;
			};
		};
	};
