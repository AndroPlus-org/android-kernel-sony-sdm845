&soc {
	qcom,pcie@0x1c00000 {
		status = "disabled";
	};

	qcom,pcie@0x1c08000 {
		status = "disabled";
	};

	pinctrl@03400000 {
		disabled-pins = <0 1 2 3 81 82 83 84>;
	};

	cam_vana_front_verg: cam_vana_front_verg {
		compatible = "regulator-fixed";
		regulator-name = "cam_vana_front_verg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&tlmm 8 0>;
	};

	cam_vana_rear_verg: cam_vana_rear_verg {
		compatible = "regulator-fixed";
		regulator-name = "cam_vana_rear_verg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&tlmm 27 0>;
	};

	gpio_keys {
		cam_focus {
			label = "cam_focus";
			gpios = <&pm8998_gpios 2 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <528>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		vol_dn {
			label = "volume_down";
			gpios = <&pm8998_gpios 5 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	fpc1145 {
		status = "ok";
		compatible = "fpc,fpc1020", "fpc1145";
		interrupt-parent = <&tlmm>;
		interrupts = <121 0x0>;
		fpc,gpio_rst    = <&tlmm 40 0x0>;
		fpc,gpio_irq    = <&tlmm 121 0x0>;
